package funcoes;

import java.util.Scanner;

public class Desafio {

	public static void main(String[] args) {
		
		Scanner ler = new Scanner(System.in);
		int num;
		
		System.out.println("Digite um número:");
		num = ler.nextInt();
		linha(num);
		
		
	}
	
	public static void linha(int num) {
		for(int x = 0; x <= num; x++) {
			System.out.println("-");
			
		}
	    
		
	}

}
